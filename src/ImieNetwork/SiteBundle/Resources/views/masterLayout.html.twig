<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}IMIE - {% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet" href="{{ asset('bundles/ImieNetworkSite/css/bootstrap-theme.min.css') }}" />
        <link rel="stylesheet" href="{{ asset('bundles/ImieNetworkSite/css/IMIE.css') }}" />
        <link rel="stylesheet" href="{{ asset('bundles/ImieNetworkSite/css/bootstrap.min.css') }}" />
        <script src="{{ asset('bundles/ImieNetworkSite/js/jquery-2.1.1.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/ImieNetworkSite/js/bootstrap.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/ImieNetworkSite/js/kendoui.core/js/kendoui.min.js') }}" type="text/javascript"></script>
        {% block HeaderJavascripts %}{% endblock %}

    </head>
    <body>
        {{ include('ImieNetworkSiteBundle:Default:header.html.twig') }}
        {{ include('ImieNetworkSiteBundle:Default:navigation.html.twig') }}            
{% block body %}
            <header class="contentHeader">
        {% block ContentHeader %}{% endblock %}
            </header>
            <main class="contentMain">
                <div class="col-md-10">
            {% block content %}{% endblock %}
                </div>
            </main>
            <footer class="contentFooter">
            {%block contentFooter %}{% endblock %}
            </footer>
{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
